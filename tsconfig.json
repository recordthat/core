{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "14.1.0",
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "node16",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
